id: nifext_109
name: Spinal cord intermediate horn motor neuron sympathetic
synonym: "Sympathetic motor neuron" []
synonym: "preganglionic sympathetic neuron" []

[Term]
id: nlx_38713
name: Spinal cord motor neuron parasympathetic
def: "Spinal cord parasympathetic motor neuron located in the sacral spinal cord" []

